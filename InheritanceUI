class Human {
  constructor(){
    this.gender = 'male';
  }
  printMyGender(){
    console.log(this.gender);
  }

}

class Person extends Human{
  constructor() {
    super();
    this.name = 'salman';
  }

  printMyName() {
    console.log(this.name);
  }
}

const pers = new Person();
pers.printMyName();
pers.printMyGender();
